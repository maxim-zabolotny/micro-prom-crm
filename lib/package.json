{
  "name": "lib",
  "version": "1.0.0",
  "main": "./src/index.ts",
  "description": "",
  "keywords": [],
  "author": "Denys Zolotarenko",
  "license": "ISC",
  "private": true,
  "scripts": {
    "test": "npm run ts:mocha test/",
    "test:int": "npm run ts:mocha test/integration/",
    "test:unit": "npm run ts:mocha test/unit/",
    "test:watch": "mocha --extension ts -r tsconfig-paths/register -r ts-node/register --watch",
    "build": "NODE_ENV=production tsc -p tsconfig.production.json",
    "build:dev": "NODE_ENV=development tsc -p tsconfig.production.json",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:errors": "eslint --quiet .",
    "ts": "tsc",
    "ts:run": "ts-node --transpileOnly --project ./tsconfig.json -r tsconfig-paths/register",
    "ts:watch": "ts-node-dev --respawn --transpile-only --project ./tsconfig.json -r tsconfig-paths/register",
    "ts:mocha": "mocha --extension ts --recursive -r tsconfig-paths/register -r ts-node/register",
    "ts:patch": "ts-patch install -s",
    "init": "npm install && npm run ts:patch",
    "pkg:precommit": "npm audit && npm run lint:errors && npm run test"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "http-errors": "^2.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "@types/moment": "^2.13.0",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.3.1",
    "@types/http-errors": "^1.8.2",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.42",
    "@types/url-join": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "chai": "^4.3.6",
    "dotenv": "^16.0.1",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "mocha": "^10.0.0",
    "nock": "^13.2.6",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "ts-patch": "^2.0.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3",
    "typescript-transform-paths": "^3.3.1"
  }
}
