{
  "name": "lib",
  "version": "1.0.0",
  "main": "./src/index.ts",
  "description": "",
  "keywords": [],
  "author": "Denys Zolotarenko",
  "license": "ISC",
  "private": true,
  "scripts": {
    "test": "npm run ts:mocha test/",
    "test:int": "npm run ts:mocha test/integration/",
    "test:unit": "npm run ts:mocha test/unit/",
    "test:watch": "mocha --extension ts -r tsconfig-paths/register -r ts-node/register --watch",
    "build": "npm run build:release",
    "build:dev": "node ./scripts/build.js --exec='npm run build-config:dev'",
    "build:release": "node ./scripts/build.js --exec='npm run build-config:release'",
    "build:watch": "webpack --watch --config ./webpack/webpack.dev.config.js --progress --color",
    "build-config:dev": "webpack --config ./webpack/webpack.dev.config.js --progress --color",
    "build-config:release": "webpack --config ./webpack/webpack.release.config.js --progress --color",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:errors": "eslint --quiet .",
    "ts": "tsc",
    "ts:errors": "tsc --noEmit",
    "ts:run": "ts-node --transpileOnly --project ./tsconfig.json -r tsconfig-paths/register",
    "ts:watch": "ts-node-dev --respawn --transpile-only --project ./tsconfig.json -r tsconfig-paths/register",
    "ts:mocha": "mocha --extension ts --recursive -r tsconfig-paths/register -r ts-node/register",
    "ts:patch": "ts-patch install -s",
    "ts:build": "NODE_ENV=production tsc -p tsconfig.production.json",
    "ts:build:dev": "NODE_ENV=development tsc -p tsconfig.production.json",
    "init": "npm install && npm run ts:patch",
    "pkg:precommit": "npm audit && npm run lint:errors && npm run test",
    "link": "echo 'cd ./dist/{libraryName} && npm link'",
    "link:microtron": "cd ./dist/microtron && npm link && echo '\n=== link name: microtron ==='"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cheerio": "^1.0.0-rc.12",
    "http-errors": "^2.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.3.1",
    "@types/http-errors": "^1.8.2",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.1.1",
    "@types/moment": "^2.13.0",
    "@types/node": "^17.0.42",
    "@types/url-join": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "babel-loader": "^8.2.5",
    "chai": "^4.3.6",
    "command-line-args": "^5.2.1",
    "dotenv": "^16.0.1",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "mocha": "^10.0.0",
    "nock": "^13.2.6",
    "terser-webpack-plugin": "^5.3.3",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "ts-patch": "^2.0.1",
    "tsconfig-paths": "^4.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.7.3",
    "typescript-transform-paths": "^3.3.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0"
  }
}
